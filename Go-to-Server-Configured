export TERM="xterm"
source "$DESTINATION/Parse-Config-File" "$CONFIGURATION"
if [ $VARIANT -eq 1 ] && [ -n "$2" ] ; then
	echo "This program is not configured to take the second argument \"$1\"!"
	echo "Terminating..."
	exit 3
elif [ $VARIANT -eq 2 ] ; then
	source "$DESTINATION/Go-to-Server-with-Expect" "$1" "$2"
else
	source "$DESTINATION/Go-to-Server" "$1"
fi
